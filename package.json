{
  "name": "re-mplus",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc --project ./",
    "build:cron": "esbuild src/scripts/analyse.ts --bundle --platform=node --outfile=dist/analyse.js",
    "build:data": "tsx src/scripts/analyse.ts public/data.json",
    "build:public": "esbuild src/browser/main.ts --bundle --minify --outfile=public/main.js",
    "dev": "esbuild src/browser/main.ts --bundle --outdir=public --watch --servedir=public",
    "start": "serve public",
    "lint": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "async": "^3.2.6",
    "tsx": "^4.19.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "typescript": "^5.6.2"
  }
}
